{
  "name": "vscode-project-context-based-menu-items",
  "private": "true",
  "version": "1.3.0",
  "publisher": "SAPOSS",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/app-studio-toolkit"
  },
  "main": "./dist/src/extension",
  "scripts": {
    "ci": "npm-run-all clean compile package",
    "clean": "rimraf dist *.vsix",
    "compile": "tsc -p ./",
    "package": "vsce package"
  },
  "dependencies": {},
  "devDependencies": {},
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "extension.capPreview",
          "when": "resourcePath in bas_project_types:cap"
        },
        {
          "command": "extension.ui5Deploy",
          "when": "resourcePath in bas_project_types:ui5"
        },
        {
          "command": "extension.ui5EditManifest",
          "when": "resourcePath in bas_project_types:ui5 && resourceFilename == manifest.json"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.capPreview",
        "title": "Preview LCAP Application"
      },
      {
        "command": "extension.ui5Deploy",
        "title": "Deploy UI5 Application"
      },
      {
        "command": "extension.ui5EditManifest",
        "title": "Edit UI5 Manifest"
      }
    ]
  }
}
